{
  "expo": {
    "name": "intanet-attendance",
    "slug": "intanet-attendance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "intanetattendance",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        [
          {
            "assets": [
              "./assets/images/background.png",
              "./assets/images/intanet.png"
            ]
          }
        ]
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/urbanist/300Light/Urbanist_300Light.ttf",
            "node_modules/@expo-google-fonts/urbanist/400Regular/Urbanist_400Regular.ttf",
            "node_modules/@expo-google-fonts/urbanist/500Medium/Urbanist_500Medium.ttf",
            "node_modules/@expo-google-fonts/urbanist/600SemiBold/Urbanist_600SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
